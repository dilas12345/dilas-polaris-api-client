{
    "name": "dilas/polaris-api-client",
    "description": "Polaris Bank API Client for PHP",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "api",
        "api-client",
        "polaris",
        "dilas"
    ],
    "autoload": {
        "psr-4": {
            "Dilas\\PolarisBank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dilas\\PolarisBank\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Che Dilas Yusuph",
            "email": "josephdilas@lovetechnigeria.com.ng",
            "homepage": "https://lovetechnigeria.com.ng"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "brokeyourbike/http-client": "^1.0",
        "brokeyourbike/resolve-uri": "^1.0",
        "brokeyourbike/http-enums": "^2.0",
        "brokeyourbike/has-source-model": "^2.0",
        "psr/simple-cache": "^1.0",
        "brokeyourbike/data-transfer-object": "^0.2"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "nesbot/carbon": "^2"
    },
    "prefer-stable": true,
   
    "version": "0.3.3",
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
